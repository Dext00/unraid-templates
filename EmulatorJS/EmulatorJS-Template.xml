<?xml version="1.0"?>
<Container version="2">
  <Name>EmulatorJS</Name>
  <Repository>linuxserver/emulatorjs:latest</Repository>
  <Registry>https://hub.docker.com/r/linuxserver/emulatorjs</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/EmulatorJS/EmulatorJS/issues</Support>
  <Project>https://github.com/EmulatorJS/EmulatorJS</Project>
  <Overview>ROM files are installed via the backend. The Backend can be accessed at:&#xD;
http://yourIP:3000/&#xD;
&#xD;
Browser support&#xD;
Only Safari for Apple devices and Chromium based browsers are supported. Other browsers might work, but have not been tested and likely never will be. Gamepads do not function on Firefox. </Overview>
  <Category>GameServers:</Category>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL/>
  <Icon>https://emulatorjs.org/favicon.ico</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1693401337</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Data" Target="/data/" Default="/mnt/user/appdata/Emulator/data" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/Emulator/data</Config>
  <Config Name="Config" Target="/config/" Default="/mnt/user/appdata/Emulator/config" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/Emulator/config</Config>
  <Config Name="WebUI" Target="80" Default="280" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">280</Config>
  <Config Name="Backend-Port" Target="3000" Default="3000" Mode="tcp" Description="" Type="Port" Display="advanced-hide" Required="true" Mask="false">3000</Config>
</Container>
